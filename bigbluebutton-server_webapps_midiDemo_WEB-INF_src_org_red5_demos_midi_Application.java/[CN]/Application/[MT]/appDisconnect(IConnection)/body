{
  if (conn.hasAttribute("midiIn")) {
    MidiDevice dev=(MidiDevice)conn.getAttribute("midiIn");
    if (dev.isOpen()) {
      dev.close();
    }
  }
  if (conn.hasAttribute("midiOut")) {
    MidiDevice dev=(MidiDevice)conn.getAttribute("midiOut");
    if (dev.isOpen()) {
      dev.close();
    }
  }
  super.appDisconnect(conn);
}
