{
  log.debug("onCallConfirmed","Init...");
  if (call != this.call) {
    log.debug("onCallConfirmed","NOT the current call.");
    return;
  }
  log.debug("onCallConfirmed","CONFIRMED/CALL.");
  changeStatus(UA_ONCALL);
  if (clip_on != null) {
    clip_on.replay();
  }
  launchMediaApplication();
}
