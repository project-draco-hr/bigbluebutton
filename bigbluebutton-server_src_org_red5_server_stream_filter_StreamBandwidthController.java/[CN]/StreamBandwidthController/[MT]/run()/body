{
  while (isStarted && providerPipe != null && consumerPipe != null) {
    try {
      IMessage message=providerPipe.pullMessage();
      if (log.isDebugEnabled()) {
        log.debug("got message: " + message);
      }
      consumerPipe.pushMessage(message);
    }
 catch (    Exception e) {
      break;
    }
  }
  isStarted=false;
}
