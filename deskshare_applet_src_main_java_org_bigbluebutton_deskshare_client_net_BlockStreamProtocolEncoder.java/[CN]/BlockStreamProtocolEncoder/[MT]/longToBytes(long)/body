{
  byte[] data=new byte[8];
  data[0]=(byte)((i >> 56) & 0xff);
  data[1]=(byte)((i >> 48) & 0xff);
  data[2]=(byte)((i >> 40) & 0xff);
  data[3]=(byte)((i >> 32) & 0xff);
  data[4]=(byte)((i >> 24) & 0xff);
  data[5]=(byte)((i >> 16) & 0xff);
  data[6]=(byte)((i >> 8) & 0xff);
  data[7]=(byte)(i & 0xff);
  return data;
}
