{
  int count=0;
  boolean found=false;
  while (count < liveMsgs.size() || !found) {
    if (liveMsgs.get(count).equals(id)) {
      liveMsgs.remove(count);
      log.debug("Found valid keep alive msg reply from bbb-apps. id [{}]",id);
      found=true;
    }
    count++;
  }
  if (!found) {
    log.info("Received valid response from bbb-apps:" + id);
  }
}
