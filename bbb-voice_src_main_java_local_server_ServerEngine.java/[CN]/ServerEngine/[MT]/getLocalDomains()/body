{
  if (server_profile.domain_names.length > 0) {
    String str="";
    for (int i=0; i < server_profile.domain_names.length - 1; i++) {
      str+=server_profile.domain_names[i] + ", ";
    }
    return str + server_profile.domain_names[server_profile.domain_names.length - 1];
  }
 else   return "";
}
