{
  try {
    processMessage=true;
    Runnable messageProcessor=new Runnable(){
      public void run(){
        while (processMessage) {
          try {
            ReceivedMessage msg=receivedMessages.take();
            processMessage(msg);
          }
 catch (          InterruptedException e) {
          }
        }
      }
    }
;
    msgProcessorExec.execute(messageProcessor);
  }
 catch (  Exception e) {
  }
}
