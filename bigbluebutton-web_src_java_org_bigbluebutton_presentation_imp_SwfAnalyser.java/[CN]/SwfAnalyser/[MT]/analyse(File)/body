{
  NuProcessBuilder pb=new NuProcessBuilder(Arrays.asList(SWFTOOLS_DIR + File.separator + "swfdump",output.getAbsolutePath()));
  SwfPageAnalyserHandler handler=new SwfPageAnalyserHandler(output);
  handler.setMaxSwfShapes(maxSwfShapes);
  pb.setProcessListener(handler);
  NuProcess process=pb.start();
  try {
    process.waitFor(60,TimeUnit.SECONDS);
  }
 catch (  InterruptedException e) {
    log.error(e.getMessage());
  }
  return handler.isConversionSuccessfull();
}
