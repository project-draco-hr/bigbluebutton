{
  File[] thumbs=thumbsDir.listFiles();
  if (thumbs.length != pageCount) {
    for (int i=0; i < pageCount; i++) {
      File thumb=new File(thumbsDir.getAbsolutePath() + File.separator + TEMP_THUMB_NAME+ "-"+ i+ ".png");
      if (!thumb.exists()) {
        copyBlankThumbnail(thumb);
      }
    }
  }
}
