{
switch (event.operation) {
case Constants.DATA_OPERATION_UPDATE_ATTRIBUTES:
    List<Object> contents=(List<Object>)event.body;
  @SuppressWarnings("unused") List<String> attributeNames=(List<String>)contents.get(0);
@SuppressWarnings("unused") Map<String,Object> oldValues=(Map<String,Object>)contents.get(1);
@SuppressWarnings("unused") Map<String,Object> newValues=(Map<String,Object>)contents.get(2);
break;
default :
log.error("Unknown data update request: " + event);
}
}
