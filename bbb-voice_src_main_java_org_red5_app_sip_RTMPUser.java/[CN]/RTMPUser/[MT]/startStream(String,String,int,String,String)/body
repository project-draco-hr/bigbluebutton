{
  logger.debug("RTMPUser startStream");
  this.publishName=publishName;
  this.playName=playName;
  createdPlayStream=false;
  startPublish=false;
  videoTs=0;
  audioTs=0;
  kt=0;
  kt2=0;
  try {
    connect(host,port,app,this);
  }
 catch (  Exception e) {
    logger.error("RTMPUser startStream exception " + e);
  }
}
