{
synchronized (this) {
    if (refCount <= 0) {
      refCount=0;
      throw new IllegalStateException("Already released buffer.  You released the buffer too many times.");
    }
    refCount--;
    if (refCount > 0) {
      return;
    }
  }
  if (disposed) {
    return;
  }
  buf.release();
synchronized (containerStack) {
    containerStack.push(this);
  }
}
