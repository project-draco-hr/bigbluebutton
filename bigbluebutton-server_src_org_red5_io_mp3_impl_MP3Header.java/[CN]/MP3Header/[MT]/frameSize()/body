{
switch (layerDescription) {
case 3:
    return (12 * getBitRate() / getSampleRate() + (paddingBit ? 1 : 0)) * 4;
case 2:
case 1:
  if (audioVersionId == 3) {
    return 144 * getBitRate() / getSampleRate() + (paddingBit ? 1 : 0);
  }
 else {
    return 72 * getBitRate() / getSampleRate() + (paddingBit ? 1 : 0);
  }
default :
return -1;
}
}
