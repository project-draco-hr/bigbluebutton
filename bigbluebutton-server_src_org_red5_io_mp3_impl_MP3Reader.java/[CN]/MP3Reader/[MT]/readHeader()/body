{
  MP3Header header=null;
  while (header == null && in.remaining() > 4) {
    try {
      header=new MP3Header(in.getInt());
    }
 catch (    IOException e) {
      log.error("MP3Reader :: readTag ::>\n",e);
      break;
    }
catch (    Exception e) {
      searchNextFrame();
    }
  }
  return header;
}
