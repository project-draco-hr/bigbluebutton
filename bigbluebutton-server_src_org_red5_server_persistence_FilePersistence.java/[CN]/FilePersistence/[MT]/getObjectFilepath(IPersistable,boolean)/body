{
  String result=path + '/' + object.getType()+ '/'+ object.getPath();
  if (!result.endsWith("/")) {
    result+='/';
  }
  if (completePath) {
    String name=object.getName();
    int pos=name.lastIndexOf('/');
    if (pos >= 0) {
      result+=name.substring(0,pos);
    }
  }
  return result;
}
