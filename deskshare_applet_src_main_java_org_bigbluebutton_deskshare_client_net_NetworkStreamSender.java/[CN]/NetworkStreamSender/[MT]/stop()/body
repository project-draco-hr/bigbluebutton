{
  System.out.println("Stopping network sender");
  for (int i=0; i < numRunningThreads; i++) {
    if (tunneling) {
      httpSenders[i].disconnect();
    }
 else {
      socketSenders[i].disconnect();
    }
  }
  stopped=true;
  executor.shutdownNow();
  httpSenders=null;
  socketSenders=null;
}
