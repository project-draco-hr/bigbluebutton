{
  String tileInfo=Integer.toString(tile.getWidth()) + "x" + Integer.toString(tile.getHeight())+ "x"+ Integer.toString(tile.getX())+ "x"+ Integer.toString(tile.getY())+ "x"+ Integer.toString(tile.getTilePosition());
  try {
    sendCaptureUpdateMessage();
    outStream.writeInt(tileInfo.length());
    outStream.writeBytes(tileInfo);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
