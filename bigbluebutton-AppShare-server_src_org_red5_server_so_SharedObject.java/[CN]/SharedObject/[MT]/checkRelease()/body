{
  if (!isPersistentObject() && listeners.isEmpty() && !isAcquired()) {
    log.info("Deleting shared object " + name + " because all clients disconnected and it is no longer acquired.");
    if (storage != null) {
      if (!storage.remove(this)) {
        log.error("Could not remove shared object.");
      }
    }
    close();
  }
}
