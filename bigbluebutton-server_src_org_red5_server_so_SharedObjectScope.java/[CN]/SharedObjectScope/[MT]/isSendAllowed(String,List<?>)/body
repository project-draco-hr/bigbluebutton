{
  for (  ISharedObjectSecurity handler : securityHandlers) {
    if (!handler.isSendAllowed(this,message,arguments)) {
      return false;
    }
  }
  final Set<ISharedObjectSecurity> handlers=getSecurityHandlers();
  if (handlers == null) {
    return true;
  }
  for (  ISharedObjectSecurity handler : handlers) {
    if (!handler.isSendAllowed(this,message,arguments)) {
      return false;
    }
  }
  return true;
}
