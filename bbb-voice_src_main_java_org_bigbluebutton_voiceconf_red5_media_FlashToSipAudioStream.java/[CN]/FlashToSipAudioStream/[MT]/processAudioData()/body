{
  while (processAudioData) {
    try {
      AudioByteData abd=audioDataQ.take();
      if (abd.status()) {
        log.debug("Flash Queue Poisoned - Dieing");
        break;
      }
      transcoder.transcode(abd,1,abd.getData().length - 1,new TranscodedAudioListener());
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
}
