{
  log.debug("SIPUser register");
  try {
    if (sipProvider != null) {
      ra=new SIPRegisterAgent(sipProvider,userProfile.fromUrl,userProfile.contactUrl,username,userProfile.realm,password,this);
      loopRegister(userProfile.expires,userProfile.expires / 2,userProfile.keepaliveTime);
    }
  }
 catch (  Exception e) {
    log.error("register: Exception:>\n" + e);
  }
}
