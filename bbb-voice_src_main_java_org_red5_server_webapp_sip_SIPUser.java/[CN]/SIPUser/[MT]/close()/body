{
  log.debug("SIPUser close1");
  try {
    hangup();
    unregister();
  }
 catch (  Exception e) {
    log.error("close: Exception:>\n" + e);
  }
  try {
    log.debug("SIPUser provider.halt");
    sipProvider.halt();
  }
 catch (  Exception e) {
    log.error("close: Exception:>\n" + e);
  }
  service=null;
}
