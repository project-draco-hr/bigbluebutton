{
  boolean present=false;
  User p=null;
synchronized (this) {
    present=users.containsKey(userid);
    if (present) {
      p=users.get(userid);
      p.setStatus(status,value);
    }
  }
  if (present) {
    UserStatusChangeMessage msg=new UserStatusChangeMessage(meetingID,recorded,userid,status,value);
    outMessageGateway.send(msg);
  }
}
