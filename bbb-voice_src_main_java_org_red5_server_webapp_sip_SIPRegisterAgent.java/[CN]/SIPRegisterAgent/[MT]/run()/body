{
  isRunning=true;
  try {
    while (loop) {
      register();
      long waitCnt=0;
      while (regInprocess) {
        Thread.sleep(1000);
        waitCnt+=1000;
      }
      if (lastRegFailed) {
        printLog("Failed Registration stop try.");
        halt();
      }
 else       Thread.sleep(renewTime * 1000 - waitCnt);
    }
  }
 catch (  Exception e) {
    printException(e);
  }
  isRunning=false;
}
