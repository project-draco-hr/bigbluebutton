{
  super.remove(name);
  String filename=path + '/' + name+ extension;
  Resource resFile=resources.getResource(filename);
  if (!resFile.exists()) {
    return true;
  }
  try {
    boolean result=resFile.getFile().delete();
    if (result) {
      checkRemoveEmptyDirectories(filename);
    }
    return result;
  }
 catch (  IOException err) {
    return false;
  }
}
