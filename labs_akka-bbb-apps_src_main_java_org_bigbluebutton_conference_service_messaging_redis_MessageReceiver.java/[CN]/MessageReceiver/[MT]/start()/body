{
  try {
    receiveMessage=true;
    final Jedis jedis=redisPool.getResource();
    Runnable messageReceiver=new Runnable(){
      public void run(){
        if (receiveMessage) {
          jedis.psubscribe(new PubSubListener(),MessagingConstants.TO_BBB_APPS_PATTERN);
        }
      }
    }
;
    msgReceiverExec.execute(messageReceiver);
  }
 catch (  Exception e) {
  }
}
