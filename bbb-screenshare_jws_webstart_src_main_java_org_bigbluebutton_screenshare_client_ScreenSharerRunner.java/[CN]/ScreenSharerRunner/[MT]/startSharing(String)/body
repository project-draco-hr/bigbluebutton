{
  try {
    String publishUrl=ssi.URL + "/" + streamId;
    System.out.println("Publishing stream [" + streamId + "] to "+ publishUrl);
    jcs.go(publishUrl,x,y,width,height);
    jcs.start();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
catch (  AWTException e) {
    e.printStackTrace();
  }
catch (  InterruptedException e) {
    e.printStackTrace();
  }
catch (  Exception e) {
    e.printStackTrace();
  }
}
