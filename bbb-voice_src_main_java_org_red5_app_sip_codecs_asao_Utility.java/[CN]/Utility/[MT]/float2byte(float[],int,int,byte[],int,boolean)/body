{
  if (flag) {
    int l=0;
    for (int j1=j; l < j1; l++) {
      int l1=(int)af[i + l];
      abyte0[k + 2 * l]=(byte)(l1 >> 8 & 0xff);
      abyte0[k + 2 * l + 1]=(byte)(l1 >> 0 & 0xff);
    }
  }
 else {
    int i1=0;
    for (int k1=j; i1 < k1; i1++) {
      int i2=(int)af[i + i1];
      abyte0[k + 2 * i1]=(byte)(i2 >> 0 & 0xff);
      abyte0[k + 2 * i1 + 1]=(byte)(i2 >> 8 & 0xff);
    }
  }
}
