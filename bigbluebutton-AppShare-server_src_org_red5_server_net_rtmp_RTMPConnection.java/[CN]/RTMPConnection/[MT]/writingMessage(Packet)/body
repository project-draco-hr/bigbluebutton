{
  if (message.getMessage() instanceof VideoData) {
    int streamId=message.getHeader().getStreamId();
    final AtomicInteger value=new AtomicInteger();
    AtomicInteger old=pendingVideos.putIfAbsent(streamId,value);
    if (old == null) {
      old=value;
    }
    old.incrementAndGet();
  }
}
