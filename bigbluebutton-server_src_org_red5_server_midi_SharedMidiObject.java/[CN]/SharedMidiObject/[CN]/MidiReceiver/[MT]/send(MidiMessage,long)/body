{
  byte[] msg=midi.getMessage();
  int len=midi.getLength();
  if (len <= 1) {
    return;
  }
  List<Object> list=new ArrayList<Object>(3);
  list.add(time);
  list.add(len);
  list.add(msg);
  so.beginUpdate();
  so.sendMessage("midi",list);
  so.endUpdate();
  String out="Midi >> Status: " + msg[0] + " Data: [";
  for (int i=1; i < len; i++) {
    out+=msg[i] + ((i == len - 1) ? "" : ",");
  }
  out+=']';
  log.debug(out);
}
