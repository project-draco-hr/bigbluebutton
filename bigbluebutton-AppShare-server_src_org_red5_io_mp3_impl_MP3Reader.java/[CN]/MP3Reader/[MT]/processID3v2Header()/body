{
  if (in.remaining() <= 10)   return;
  int start=in.position();
  byte a, b, c;
  a=in.get();
  b=in.get();
  c=in.get();
  if (a != 'I' || b != 'D' || c != '3') {
    in.position(start);
    return;
  }
  in.skip(3);
  int size=(in.get() & 0x7f) << 21 | (in.get() & 0x7f) << 14 | (in.get() & 0x7f) << 7 | (in.get() & 0x7f);
  in.skip(size);
}
