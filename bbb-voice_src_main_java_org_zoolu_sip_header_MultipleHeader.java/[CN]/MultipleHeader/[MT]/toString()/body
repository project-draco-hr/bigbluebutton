{
  if (compact) {
    String str=name + ": ";
    for (int i=0; i < values.size() - 1; i++)     str+=values.elementAt(i) + ", ";
    if (values.size() > 0)     str+=values.elementAt(values.size() - 1);
    return str + "\r\n";
  }
 else {
    String str="";
    for (int i=0; i < values.size(); i++)     str+=name + ": " + values.elementAt(i)+ "\r\n";
    return str;
  }
}
