{
  StringBuilder sb=new StringBuilder("Service: ");
  sb.append(serviceName);
  sb.append(" Method: ");
  sb.append(serviceMethodName);
  if (arguments != null) {
    sb.append(" Num Params: ");
    sb.append(arguments.length);
    for (int i=0; i < arguments.length; i++) {
      sb.append(i);
      sb.append(": ");
      sb.append(arguments[i]);
    }
  }
 else {
    sb.append(" No params");
  }
  return sb.toString();
}
