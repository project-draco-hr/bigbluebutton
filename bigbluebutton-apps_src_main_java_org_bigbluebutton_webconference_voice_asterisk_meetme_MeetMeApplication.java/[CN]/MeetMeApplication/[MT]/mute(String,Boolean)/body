{
  log.debug("Mute: $conference $mute");
  MeetMeRoom room=getMeetMeRoom(conference);
  if (room == null)   return;
  Collection<MeetMeUser> users=room.getUsers();
  for (Iterator<MeetMeUser> it=users.iterator(); it.hasNext(); ) {
    MeetMeUser muser=(MeetMeUser)it.next();
    if (mute) {
      muser.mute();
    }
 else {
      muser.unmute();
    }
  }
}
