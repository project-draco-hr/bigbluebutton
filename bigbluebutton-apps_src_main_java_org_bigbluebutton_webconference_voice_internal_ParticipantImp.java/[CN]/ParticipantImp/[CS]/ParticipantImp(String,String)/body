{
  this.id=id;
  this.name=name;
  Matcher matcher;
  matcher=CALLERNAME_PATTERN.matcher(name);
  if (matcher.matches()) {
    this.userID=matcher.group(1).trim();
  }
 else {
    this.userID=name;
  }
  System.out.println("**** fsID=[" + id + "] userID=["+ userID+ "] name=["+ name+ "] **************************** ");
}
