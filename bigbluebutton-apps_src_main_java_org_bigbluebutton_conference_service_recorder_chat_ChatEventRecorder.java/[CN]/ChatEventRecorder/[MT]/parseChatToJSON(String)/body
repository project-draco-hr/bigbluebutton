{
  String json="{ ";
  String[] chat_attribs=message.trim().split("\\|",-1);
  json+="\"module\":\"chat\", ";
  json+="\"event\":\"new_message\", ";
  json+="\"user\":\"" + chat_attribs[1] + "\", ";
  json+="\"text\":\"" + chat_attribs[0] + "\", ";
  json+="\"language\":\"" + chat_attribs[4] + "\", ";
  json+="\"color\":\"" + chat_attribs[2] + "\" }";
  return json;
}
