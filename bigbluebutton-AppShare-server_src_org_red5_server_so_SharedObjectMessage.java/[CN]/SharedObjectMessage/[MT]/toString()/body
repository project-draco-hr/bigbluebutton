{
  final StringBuffer sb=new StringBuffer();
  sb.append("SharedObjectMessage: ").append(name).append(" { ");
  final Iterator<ISharedObjectEvent> it=events.iterator();
  while (it.hasNext()) {
    sb.append(it.next());
    if (it.hasNext()) {
      sb.append(" , ");
    }
  }
  sb.append(" } ");
  return sb.toString();
}
