{
  if (channelId <= 63) {
    out.put((byte)((headerSize << 6) + channelId));
  }
 else   if (channelId <= 320) {
    out.put((byte)(headerSize << 6));
    out.put((byte)(channelId - 64));
  }
 else {
    out.put((byte)((headerSize << 6) | 1));
    channelId-=64;
    out.put((byte)(channelId & 0xff));
    out.put((byte)(channelId >> 8));
  }
}
