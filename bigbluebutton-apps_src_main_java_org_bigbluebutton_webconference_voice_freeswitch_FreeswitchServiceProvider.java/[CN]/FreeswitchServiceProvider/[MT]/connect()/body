{
  log.info("Logging in as [" + connection.getPassword() + "] to ["+ connection.getHostname()+ ":"+ connection.getPort()+ "]");
  try {
    connection.connect();
    return true;
  }
 catch (  InboundConnectionFailure e) {
    log.error("Connect to FreeSwitch ESL socket failed",e);
  }
  return false;
}
