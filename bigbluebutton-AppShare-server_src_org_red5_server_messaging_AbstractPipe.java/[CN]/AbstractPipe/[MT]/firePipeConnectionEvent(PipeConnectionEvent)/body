{
  for (  IPipeConnectionListener element : listeners) {
    try {
      element.onPipeConnectionEvent(event);
    }
 catch (    Throwable t) {
      log.error("exception when handling pipe connection event",t);
    }
  }
  for (  Runnable task : event.getTaskList()) {
    try {
      task.run();
    }
 catch (    Throwable t) {
    }
  }
  event.getTaskList().clear();
}
