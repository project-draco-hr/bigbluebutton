{
  IMessage message;
  do {
    try {
      message=msgIn.pullMessage();
    }
 catch (    IOException err) {
      log.error("Error while pulling message.",err);
      message=null;
    }
    if (message == null) {
      return null;
    }
  }
 while (!(message instanceof RTMPMessage));
  return (RTMPMessage)message;
}
