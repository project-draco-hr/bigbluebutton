{
  try {
    String line=in.getString(Charset.forName("UTF-8").newDecoder());
    if (!session.containsAttribute(ROOM)) {
      session.setAttribute(ROOM,line.trim());
    }
 else {
      session.setAttribute(VIDEO_INFO,line.trim());
    }
  }
 catch (  CharacterCodingException e1) {
    e1.printStackTrace();
  }
}
