{
  int start=in.position();
  byte previous=0;
  while (in.hasRemaining()) {
    byte current=in.get();
    if (previous == '\r' && current == '\n') {
      int position=in.position();
      int limit=in.limit();
      try {
        in.position(start);
        in.limit(position);
        parseLine(in.slice(),decoderState);
      }
  finally {
        in.position(position);
        in.limit(limit);
      }
      return true;
    }
    previous=current;
  }
  in.position(start);
  return false;
}
