{
  EncoderRunner runner=new EncoderRunner();
  int count=0;
  while (count < 100) {
    try {
      runner.socket=runner.serverSocket.accept();
      runner.imageEncoder.acceptImage(runner.socket);
      count++;
    }
 catch (    Exception e) {
      e.printStackTrace(System.out);
    }
  }
  runner.imageEncoder.closeStreams();
}
