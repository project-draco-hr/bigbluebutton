{
  if (meetMeSOs.containsKey(room)) {
    Collection<MeetMeUser> currentUsers=voiceService.getUsers(room);
    log.info("MeetMe::roomListener - There are [" + currentUsers.size() + "] in room = ["+ room+ "]");
    for (Iterator it=currentUsers.iterator(); it.hasNext(); ) {
      MeetMeUser oneUser=(MeetMeUser)it.next();
      log.info("MeetMe::roomListener - Looking at userid = [" + oneUser.getUserNumber() + "] in room = ["+ oneUser.getRoom().getRoomNumber()+ "]");
      if (oneUser.getUserNumber().intValue() == userId.intValue()) {
        log.info("MeetMe::roomListener - Found userid = [" + userId + "] in room = ["+ room+ "]");
        return oneUser;
      }
    }
  }
  return null;
}
