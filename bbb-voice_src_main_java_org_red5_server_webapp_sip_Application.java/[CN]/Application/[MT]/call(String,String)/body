{
  loginfo("Red5SIP Call " + destination);
  SIPUser sipUser=sipManager.getSIPUser(uid);
  if (sipUser != null) {
    loginfo("Red5SIP Call found user " + uid + " making call to "+ destination);
    sipUser.call(destination);
  }
 else {
    loginfo("Red5SIP Call found user " + uid + " making call to "+ destination);
  }
}
