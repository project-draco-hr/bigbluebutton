{
  File file=null;
  try {
    log.info("getVODProviderFile scope path: " + scope.getContextPath() + " name: "+ name);
    file=getStreamFile(scope,name);
  }
 catch (  IOException e) {
    log.error("Problem getting file: " + name);
  }
  if (file == null || !file.exists()) {
    return null;
  }
  return file;
}
