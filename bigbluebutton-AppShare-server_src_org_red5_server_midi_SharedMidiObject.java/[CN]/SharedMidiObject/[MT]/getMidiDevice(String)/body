{
  MidiDevice.Info[] info=MidiSystem.getMidiDeviceInfo();
  for (  Info element : info) {
    if (element.getName().equals(name)) {
      try {
        return MidiSystem.getMidiDevice(element);
      }
 catch (      MidiUnavailableException e) {
        log.error("{}",e);
      }
    }
  }
  return null;
}
