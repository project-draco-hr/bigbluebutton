{
  startBroadcast=true;
  startBroadcastRunner=new Runnable(){
    public void run(){
      while (startBroadcast) {
        captureScreen();
      }
      System.out.println("*******************Stopped screen capture. !!!!!!!!!!!!!!!!!!!");
    }
  }
;
  startBroadcastExec.execute(startBroadcastRunner);
}
