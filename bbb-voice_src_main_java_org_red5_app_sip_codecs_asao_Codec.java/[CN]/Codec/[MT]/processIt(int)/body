{
  if (i1 == 0) {
    return -1;
  }
  int j1=0;
  if ((i1 & 0xffff0000) != 0) {
    j1=16;
    i1>>=16;
  }
  if ((i1 & 0xff00) != 0) {
    j1+=8;
    i1>>=8;
  }
  if ((i1 & 0xf0) != 0) {
    j1+=4;
    i1>>=4;
  }
  if ((i1 & 0xc) != 0) {
    j1+=2;
    i1>>=2;
  }
  if ((i1 & 2) != 0) {
    j1++;
  }
  return j1;
}
