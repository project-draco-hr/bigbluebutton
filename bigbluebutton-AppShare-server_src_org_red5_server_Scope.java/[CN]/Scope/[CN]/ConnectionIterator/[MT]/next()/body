{
  if (connIterator == null || !connIterator.hasNext()) {
    if (!setIterator.hasNext()) {
      throw new NoSuchElementException();
    }
    connIterator=setIterator.next().iterator();
    while (!connIterator.hasNext()) {
      if (!setIterator.hasNext()) {
        throw new NoSuchElementException();
      }
      connIterator=setIterator.next().iterator();
    }
  }
  current=connIterator.next();
  return current;
}
