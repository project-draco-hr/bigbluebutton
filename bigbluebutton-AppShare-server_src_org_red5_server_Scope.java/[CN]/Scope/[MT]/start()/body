{
  boolean result=false;
  if (enabled && !running) {
    if (hasHandler()) {
      try {
        if (handler != null) {
          result=handler.start(this);
        }
      }
 catch (      Throwable e) {
        log.error("Could not start scope {}. {}",this,e);
      }
    }
 else {
      log.debug("Scope {} has no handler, allowing start.",this);
      result=true;
    }
    running=result;
  }
  return result;
}
