{
  BufferedImage tile=capturedSreen.getSubimage(getX(),getY(),getWidth(),getHeight());
  long oldsum;
  oldsum=checksum.getValue();
  calcChecksum(tile);
  if (oldsum != checksum.getValue()) {
    image=null;
    this.image=tile;
    version++;
    dirty=true;
  }
 else {
    tile=null;
    dirty=false;
  }
}
