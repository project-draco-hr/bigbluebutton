{
  final int numberOfResults=getNumberOfResults();
  final List<SpeechResult> results=new ArrayList<SpeechResult>(numberOfResults);
  for (int i=0; i < numberOfResults; i++) {
    SpeechResult result=new SpeechResult(Integer.valueOf(agiReply.getAttribute("score" + i)),agiReply.getAttribute("text" + i),agiReply.getAttribute("grammar" + i));
    results.add(result);
  }
  return results;
}
