{
  int payloadId;
  String rtpmap;
  Codec sipCodec=null;
  rtpmap=negotiatedSDP.getMediaDescriptor(Codec.MEDIA_TYPE_AUDIO).getAttribute(Codec.ATTRIBUTE_RTPMAP).getAttributeValue();
  if (!rtpmap.isEmpty()) {
    payloadId=Integer.parseInt(rtpmap.substring(0,rtpmap.indexOf(" ")));
    sipCodec=CodecFactory.getInstance().getSIPAudioCodec(payloadId);
    if (sipCodec == null) {
    }
 else {
    }
  }
  return sipCodec;
}
