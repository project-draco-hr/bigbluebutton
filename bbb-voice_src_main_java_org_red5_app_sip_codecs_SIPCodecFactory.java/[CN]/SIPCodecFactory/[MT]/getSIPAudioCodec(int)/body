{
  SIPCodec sipCodec;
  printLog("getSIPAudioCodec","Init...");
  printLog("getSIPAudioCodec","codecId = [" + codecId + "].");
switch (codecId) {
case audioCodecPCMU:
    sipCodec=new SIPCodecPCMU();
  break;
case audioCodecPCMA:
sipCodec=new SIPCodecPCMA();
break;
case audioCodecG729:
sipCodec=new SIPCodecG729();
break;
case audioCodeciLBC:
sipCodec=new SIPCodeciLBC();
break;
default :
sipCodec=null;
}
if (sipCodec != null) {
printLog("getSIPAudioCodec","codecId = [" + sipCodec.getCodecId() + "], codecName =  ["+ sipCodec.getCodecName()+ "].");
}
return sipCodec;
}
