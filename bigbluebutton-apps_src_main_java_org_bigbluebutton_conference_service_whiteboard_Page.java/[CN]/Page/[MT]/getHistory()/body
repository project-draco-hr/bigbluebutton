{
  List<Object[]> graphics=new ArrayList<Object[]>();
  for (  WBGraphic g : graphicObjs.values()) {
    graphics.add(g.toObjectArray());
  }
  Object[] isGridArray=new Object[1];
  isGridArray[0]=isGrid;
  graphics.add(isGridArray);
