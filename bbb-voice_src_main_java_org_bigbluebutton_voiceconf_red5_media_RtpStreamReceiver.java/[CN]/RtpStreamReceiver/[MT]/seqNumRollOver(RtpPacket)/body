{
  boolean rolledOver=lastSequenceNumber - rtpPacket.getSeqNum() > 65000;
  if (rolledOver) {
    log.debug("Packet rolling over seqNum[rtpSeqNum=" + rtpPacket.getSeqNum() + ",lastSeqNum="+ lastSequenceNumber+ "][rtpTS="+ rtpPacket.getTimestamp()+ ",lastTS="+ lastPacketTimestamp+ "][port="+ rtpSocket.getDatagramSocket().getLocalPort()+ "]");
  }
  return rolledOver;
}
